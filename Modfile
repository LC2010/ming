module.exports = {
    tasks: {
        rm : {
            target: 'dist'
        },
        mkdir: {
            target: 'dist'
        },
        compile: {
            source: "module.js",
            dest: "./dist/module.js",
            exclude: "jquery",
            baseUrl: "./src"
        }
    },
    targets : {
        dist: "rm mkdir compile"
    }
};